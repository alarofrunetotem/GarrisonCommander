package-as: GarrisonCommander
enable-nolib-creation: no

externals:
	libs/LibInit:
		url: git://git.curseforge.net/wow/libinit/mainline.git/LibInit

	libs/Ace3:
		url: svn://svn.wowace.com/wow/ace3/mainline/trunk
		tag: latest

	libs/LibItemUpgradeInfo-1.0:
		url: git://git.wowace.com/wow/libitemupgradeinfo-1-0/mainline.git
		tag: latest

move-folders:
	GarrisonCommander/GarrisonCommander-Broker: GarrisonCommander-Broker

manual-changelog:
		filename: CHANGELOG.txt
		markup-type: textile

ignore:
- .buildpath
- .project
- Debug.lua
- info.txt
- wowhead.sh